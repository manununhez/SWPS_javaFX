<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" style="-fx-background-color: lightslategrey"
          xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="pl.swps.controller.Experiment">
    <columnConstraints>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="-Infinity"/>
    </rowConstraints>
    <VBox fillWidth="false">
        <Separator/>
        <Label fx:id="labelTest" alignment="CENTER" contentDisplay="CENTER" prefWidth="600.0" textAlignment="CENTER"
               wrapText="true">
            <font>
                <Font size="35.0"/>
            </font>
            <VBox.margin>
                <Insets bottom="40.0" top="40.0"/>
            </VBox.margin>
        </Label>
        <Separator prefWidth="200.0"/>
    </VBox>
</GridPane>
