<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="pl.swps.controller.ExperimentOverview">
    <GridPane alignment="CENTER" hgap="10" prefHeight="400.0" prefWidth="600.0" vgap="10" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
            <ColumnConstraints/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints/>
            <RowConstraints minHeight="10.0" prefHeight="30.0"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        </rowConstraints>
        <Button mnemonicParsing="false" onAction="#handleNewExperiment" text="New experiment"/>
        <Button mnemonicParsing="false" onAction="#handleSeePreviousExperiment" prefHeight="27.0" prefWidth="117.0"
                text="See previous" GridPane.rowIndex="2"/>
        <Label alignment="CENTER" minHeight="-Infinity" minWidth="-Infinity" text="or" GridPane.halignment="CENTER"
               GridPane.rowIndex="1"/>
    </GridPane>
</AnchorPane>
